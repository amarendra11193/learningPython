def count_occurrence(my_list, keyword):
    x=0
    for item in my_list:
        if item == keyword:
            x+=1
    return x

list_temp = input("Enter the list separated by commas(,): ")
my_list = list_temp.split(",")
#my_list = [int(num) for num in my_list_temp]
print(my_list)
keyword = input("Enter the keyword for which we need to find the number of occurences: ")

x = count_occurrence(my_list, keyword)
print("Result is: ", x)