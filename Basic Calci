def arithmetic_operations(num1, num2, operator):
    if (operator == "+"):
        return num1+num2
    elif (operator == "-"):
        return num1-num2
    elif (operator == "*"):
        return num1*num2
    elif (operator == "/"):
        return num1/num2
    else:
        return "Unrecognized Operator for this Program!"

num1 = float(input("Enter the first Number: "))
num2 = float(input("Enter the second Number: "))
operator = input("Enter the Operator | +, -, * or /: ")
result = arithmetic_operations(num1, num2, operator)
print("Result is: ", result)